<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Supremecarwash" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="colorPrimary">@color/principal</item>
        <item name="colorSecondary">@color/white</item>
    </style>

    <style name="Theme.Supremecarwash" parent="Base.Theme.Supremecarwash" />

    <style name="CustomDialogTheme" parent="ThemeOverlay.MaterialComponents.Dialog">
            <item name="colorPrimary">@color/principal</item>
    </style>

    <!-- Base application theme. -->
    <style name="Theme.CollapsingToolbar" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/principal</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/principal</item>
        <!-- Customize your theme here. -->
    </style>

</resources>